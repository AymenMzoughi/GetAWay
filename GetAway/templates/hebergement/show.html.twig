    <table class="table">
        <tbody>
            <tr>
                <th>Referance</th>
                <td>{{ hebergement.referance }}</td>
            </tr>
            <tr>
                <th>Paye</th>
                <td>{{ hebergement.paye }}</td>
            </tr>
            <tr>
                <th>Adress</th>
                <td>{{ hebergement.adress }}</td>
            </tr>
            <tr>
                <th>Prix</th>
